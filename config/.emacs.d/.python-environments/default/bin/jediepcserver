#!/home/guo/.emacs.d/.python-environments/default/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'jediepcserver==0.2.7','console_scripts','jediepcserver'
__requires__ = 'jediepcserver==0.2.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('jediepcserver==0.2.7', 'console_scripts', 'jediepcserver')()
    )
