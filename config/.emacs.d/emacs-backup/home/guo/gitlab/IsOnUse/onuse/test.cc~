#include <iostream>
#include <vector>
#include <boost/circular_buffer.hpp>

using namespace std;

int main() {
  auto buffer = boost::circular_buffer<float>(6);
  buffer.push_back(1);
  cerr << buffer.size() << '\n';
  auto vec = vector<float>(0);
  vec.reserve(6);
  cerr << vec.size() << '\n';
  return 0;
}
