
from __future__ import absolute_import
from __future__ import division
from __future__ import print_function

import db

from edfplus import Edfplus


def parse_edf(edf_path):
    filepath = db.DB[edf_path]
    with open(filepath, "rb") as f:
        raw = f.read()
    edf = Edfplus(raw)
    db.insert_to_db(edf_path, "num_signals",  edf.num_signals)
    db.insert_to_db(edf_path, "signal_freqs", edf.signal_freqs)
    db.insert_to_db(edf_path, "starttime",    edf.starttime)
    db.insert_to_db(edf_path, "signal_names", edf.signal_names)
    db.insert_to_db(edf_path, "signals",      edf.signals)
    return True

























