#!/bin/bash

data_dir="/home/guo/data/quora/t2t/data"
model_dir="/home/guo/data/quora/t2t/model"
usr_dir="/home/guo/quora/quora"
t2t-trainer \
    --t2t_usr_dir=$usr_dir \
    --generate_data \
    --data_dir=$data_dir \
    --output_dir=$model_dir \
    --problem=quora_insincere_question \
    --model=basic_fc_relu \
    --hparams_set=basic_fc_small \
    --train_steps=10000 \
    --eval_steps=100
	    
	    



