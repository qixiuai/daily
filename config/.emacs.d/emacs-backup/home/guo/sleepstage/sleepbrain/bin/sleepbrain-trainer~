#!/bin/bash

t2t-trainer \
    --problem=sleepedfx \
    --data_dir=~/data/sleepstage/sleepbrain/sleep-edfx \
    --tmp_dir=~/data/sleepstage/sleepbrain/tmp \
    --model=sleepbrain \
    --hparams_set=sleepbrain_quick \
    --output_dir=~/data/sleepstage/sleepbrain/output \
    --train_steps=10 \
    --eval_steps=1
