package sleepstage; // machine learning signal sample

message Subject {
  Version version = 1;
  string anno = 2;
  
  int32  num_signal = 3;
  repeated string signal_name = 4;
  
  int32 num_epoch = 5;
  repeated Epoch epoch = 6;
}

message Epoch {
  int32  index = 1;
  string label = 2;
  Timestamp timestamp = 3;
  repeated Segment segment = 4;
}

message Segment {
  int32 num_value = 1;
  repeated int32 value = 2;
}

message Version {
  int32 major  = 1 [default = 0];
  int32 minior = 2 [default = 1];
}