
#include <cpprest/json.h>

#include <string>
#include <vector>

std::vector<int> pool;

static std::vector<int> Init() {
  return pool;
}

int main() {
  ucout << "Hello\n";
  Init();
  ucout << pool.size() << '\n';
  web::json::value v;
  v["1"] = 1;
  web::json::object a = v.as_object();

  using namespace web;
  
  json::value chart_data;
  chart_data["hr"] = json::value::array({1,2,3,4,5,6});
  // report/chart/hr/epoch/min,max

  std::vector<int> data;
  data(10);
  
  return 0;
}
