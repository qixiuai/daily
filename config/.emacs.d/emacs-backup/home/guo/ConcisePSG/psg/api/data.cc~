
#include "psg/api/data.h"

namespace psg {
  namespace comm {
    std::string to_string(const Events& events) {
      std::string msg;
      for (auto evt_p : events) {
	auto evt = evt_p.second;
	msg.append(std::to_string(evt.id) + ',');
	msg.append(std::to_string(evt.start) + ',');
	msg.append(std::to_string(evt.end) + ',');
	msg.append(evt.signal_name + '\n');
      }
      return msg;
    }

  }
}
