#include <iostream>
#include <vector>

#include "psg/lib/resample/resample.h"

using namespace std;

int main ()
{
  vector<int> input{1, 2, 3, 4, 5, 6, 7, 8, 9};
  vector<float> output;
  output = resample(input, 2, 3);
  for ( int i = 0, n = output.size(); i < n; i++ )
    cout << output[i] << " ";
  cout<<endl;
  cout << input.size() << ',' << output.size() << endl;
  return 0;
}
