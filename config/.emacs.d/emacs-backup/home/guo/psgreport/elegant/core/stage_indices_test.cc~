
#include "elegant/core/stage_indices.h"

#include <cassert>

int main() {
  const std::vector<int> \
    stages{0,0,0,0,0,1,2,1,1,1,1,2,2,2,2,2,2,2,3,3,3,1,1,2,2,2,2,0,0};

  int sleep_onset = 5;
  int close_id = 2;
  int open_id = 28
  assert (elegant::core::GetsleepOnsetEpochID(stages, close_id) == 5);
  assert (elegant::core::GetStageDurationInMinutes(stages, 5, 28, 1) == 3.5);
  assert (elegant::core::GetStageLatencyInMinutes(stages, 2, 1) == 1.5);
  
  return 0;
}
