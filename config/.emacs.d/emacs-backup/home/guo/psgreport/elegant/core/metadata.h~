    int stage;
  };

  struct Event {
    Index Start;
    Index end;
  };
  
  struct Heart {
    std::vector<int> ipks;
    std::vector<float> hrs;
  };

  struct Leg {
    
  };

  struct Resp {
    
  };

  struct Spo2 {
    
  };

  struct Pose {
    
  };  
  
    
  struct Epoch {
    typedef std::vector<Event> EventVec;
    EpochID epoch_id;
    int stage;
    int pose;
    std::vector<float> heart_rates;
    EventVec plms;
    EventVec oa_vec;
    EventVec ca_vec;
    EventVec ma_vec;
    EventVec hyp_vec;
    EventVec desat_vec;
  };
  
  typedef std::vector<Epoch> EpochVec;




