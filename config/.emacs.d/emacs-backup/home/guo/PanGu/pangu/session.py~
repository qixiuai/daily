
from __future__ import absolute_import
from __future__ import division
from __future__ import print_function

from pangu.registry import _GRAPH


class Session(object):
    """ User related """

    def __init__(self):
        """
        graph: logic graph
        inputs: Graph front inputs
        """
        self._id = ""
        self._graph = _GRAPH
        self._db = {}

    @property
    def db(self):
        return self._db

    def run(self, fetch):
        graph_fn = self.generate_graph_fn(fetch=fetch)
        val = graph_fn(self.db)
        return val
    
    def generate_graph_fn(fetch=None):
        pass

    
    
    
