
from __future__ import absolute_import
from __future__ import division
from __future__ import print_function


class Graph(object):
    """ store relations between messages and ops """
    
    def __init__(self):
        self._messages = [] # names
        self._ops = [] # names


    @property
    def messages(self):
        return self._messages


    @property
    def ops(self):
        return self._ops


    def get_execuate_tree(self, fetches=[]):
        pass
    
    

    
    
