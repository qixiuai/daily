
from __future__ import absolute_import
from __future__ import division
from __future__ import print_function


def guard(func, **kwargs):
    print(func)
    print(**kwargs)
    return func


@guard
def add(id="1", a=10):
    return a + 1


if __name__ == '__main__':
    print("result: {}".format(add("1",a=1)))
