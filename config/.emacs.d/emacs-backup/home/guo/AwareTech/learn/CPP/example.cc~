#include <iostream>
#include <string>
#include <sstream>
#include <vector>
#include <algorithm>



using namespace std;

namespace Test {
  namespace Test2 {
    int add(int a, int b) {
      return a+b;
    }
  }

}

template <int order>
int remove_order() {
  return  1 + order;
}

int I_can_remove_order(const int order) {
  return remove_order<order>();
}

int main() {
  
  const int n = 0;
  cout << I_can_remove_order(n) << endl;


  
  return 0;
}
