#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

// TODO

template <class T>
void print_vector(const vector<T> vec) {
  for_each(vec.begin(), vec.end(), [](const T& x)
	   { cout << x << '\t'; });
  cout << endl;
}

int main() {
  vector<int> vec;
  vec.push_back(1);
  vec.push_back(2);
  print_vector(vec);
  vec.clear();
  print_vector(vec);
  return 0;
}

