#include <iostream>
#include <vector>
#include <string>

#include "BCG.h"

int main() {
  std::vector<int> data = {1,2,3,4,5};
  std::vector<BCG::BCGResult> results(5);
  
  int Fz = 200;
  int heart_window_size = 0.5*Fz;
  int repiratory_window_size = 3*Fz;
  bool reset = false;
  
  BCG::BCGGraph bcg(Fz, heart_window_size, repiratory_window_size);
  for (auto num: data) {
    auto ret = bcg.run(data, reset);
    results.push_back(ret);
  }

  for (auto ret: results)
    std::cout << ret << std::endl;
  
  return 0;
}
