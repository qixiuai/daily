from django.db import models

# Create your models here.

class VisualRecord(models.Model):
    data      = models.CommaSeparatedIntegerField()#max_length=1024*1024*1024)
    data_type = models.CharField(max_length=200)
    filter    = models.CharField(max_length=200)
    pub_date  = models.DateTimeField("Data uploaded time")

    def __str__(self):
        return "VisualRecord: length:%s\tdata_type:%s\tfilter:%s\tpub_date\n" % (
            len(self.data), data_type, filter, pub_date)
    
    
