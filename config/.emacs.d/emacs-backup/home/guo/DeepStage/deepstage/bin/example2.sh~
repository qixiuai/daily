#!/bin/bash

rm -rf ~/data/tmp/ds_data2
rm -rf ~/data/sleepstage/sleepbrain/tmp/ds_train2

export PYTHONPATH="/home/guo/DeepStage"
ds-trainer \
    --generate_data \
    --data_dir=~/data/tmp/ds_data2 \
    --output_dir=~/data/sleepstage/sleepbrain/tmp/ds_train2/deepstage \
    --problem=psg_isruc \
    --model=shake_shake \
    --hparams_set=shakeshake_quick \
    --train_steps=500000 \
    --eval_steps=100
