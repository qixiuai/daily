#!/bin/bash

rm -rf ~/data/tmp
rm -rf ~/data/sleepstage/sleepbrain/tmp/

export PYTHONPATH="/home/guo/DeepStage"
ds-trainer \
    --generate_data \
    --data_dir=~/data/tmp/ds_data \
    --output_dir=~/data/sleepstage/sleepbrain/tmp/ds_train/deepstage \
    --problem=psg_isruc \
    --model=shake_shake \
    --hparams_set=shakeshake_small \
    --train_steps=5000 \
    --eval_steps=1
