#!/bin/bash

# data dir
#rm -rf ~/data/tmp/ds_data
# model dir
rm -rf ~/data/sleepstage/sleepbrain/tmp/ds_train/deepstage

export PYTHONPATH="/home/guo/DeepStage"
ds-trainer \
    --tfdbg=True \
    --generate_data \
    --data_dir=~/data/tmp/ds_data \
    --output_dir=~/data/sleepstage/sleepbrain/tmp/ds_train/deepstage \
    --problem=stage_isruc \
    --model=demo_model \
    --hparams_set=demomodel_small \
    --train_steps=500000 \
    --eval_steps=100000

#    --worker_gpu=1
