
d = dict()
s = set()
key_ind = 1
out = open("result.txt", 'w')
with open("1.txt") as file:
    for line in file:
        line = line.strip()
        for num_str in line:
            num = int(num_str)
            if num not in s:
                s.add(num)
                d[key_ind] = num
                key_ind += 1
                out.write("{}:{}\n".format(key_ind, num))
out.close()
print(d)





