

if __name__ == '__main__':
    filepath= 'content__RR30.txt'
    outfile = 'parsed_' +filepath
    data_dir = 'data/'
    out = open(data_dir+outfile, 'w')
    with open(data_dir+filepath, 'rb') as file:
        for line in file:
            line = line[18:-7]
            data = line.decode('ascii').split(',')
            try:
                data = list(map(int, data))
            except:
                print(line)
                continue
            for val in data:
                out.write(str(val)+'\n')
