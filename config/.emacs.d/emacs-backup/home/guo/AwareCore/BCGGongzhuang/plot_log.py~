import matplotlib.pyplot as plt
import numpy as np


def plot_data(data):
    plt.plot(data[20001:,0])
#    plt.plot(data[:,1])
    plt.show()

def load_data(file_path):
    return np.loadtxt(file_path, delimiter=',')

def main(file_path):
    data = load_data(file_path)
    plot_data(data)

if __name__ == '__main__':
    file_path = "log"
    main(file_path)
