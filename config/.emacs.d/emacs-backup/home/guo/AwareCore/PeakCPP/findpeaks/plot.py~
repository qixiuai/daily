import matplotlib.pyplot as plt
import numpy as np

def main(file):
    with open(file) as f:
        signal, ipks = f.read().strip().split('\n');
    print(signal)
    print(ipks)
    signal = signal.strip().split(',')[:-1]
    signal = np.asarray(list(map(int, signal)))
    ipks = ipks.strip().split(',')[:-1]
    ipks = np.asarray(list(map(int, ipks)))
    plt.plot(signal, 'r')
    plt.plot(ipks, signal[ipks], 'o')
    plt.show()

if __name__ == '__main__':
    file = 'txt'
    main(file)
