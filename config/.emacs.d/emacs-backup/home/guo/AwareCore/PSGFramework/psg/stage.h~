#ifndef _PHYSIO_STAGE_
#define _PHYSIO_STAGE_

#include "dsa.h"
#include "utils.h"

namespace physio {
  
  class StageJudge {
  public:
    StageJudge() = default;
    StageJudge(EventQueues<int>& event_queues) {
      this->event_queues = event_queues;
    }
    int run(int eeg_f1_m2, int eeg_c3_m2, int eeg_c4_m1,
	    int eeg_f2_m1, int eeg_o1_m2, int eeg_o2_m1,
	    int eog_e2_m2, int eog_e1_m2, int emg_pos_neg) {
      return 0;
    }
  private:
    EventQueues<int>& event_queues;
  };  

}; //end namespace physio

#endif
