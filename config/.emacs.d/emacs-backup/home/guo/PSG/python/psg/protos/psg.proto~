
syntax = "proto3";

package psg;

service ReportService {
  rpc ReportRequest(EdfRequest) returns (SummaryReply) {}
}

message EdfRequest {
  string data = 1;
}

message SummaryReply {
  repeated EpochSummary epoch = 1;
}

message EpochSummary {
  int32 stage = 1;
  HeartSummary heart = 2;
  //  Spo2Summary  spao = 3;
  //  BodySummary  body = 4;
}

message LightSummary {
  bool is_close = 3;
  string event = 1;
}

message SleepSummary {
  repeated int32 stage = 1;
}

message HeartSummary {
  repeated float hr = 1;
}

message RespSummary {
  repeated float rr = 1;
}

message Spo2Summary {
  repeated float spao2 = 1;
}

message BodySummary {
  repeated int32 body = 1;
}