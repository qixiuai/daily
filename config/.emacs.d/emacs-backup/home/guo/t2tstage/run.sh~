#!/bin/bash

root_dir=/home/guo/data/sleepstage
data_dir=${root_dir}/t2t_data
model_dir=${root_dir}/t2t_model
#rm -rf ${data_dir}
rm -rf ${model_dir}

usr_dir=/home/guo/t2tstage/stage

t2t-trainer \
    --t2t_usr_dir=${usr_dir} \
    --tfdbg=False \
    --generate_data \
    --problem=stage_isruc \
    --data_dir=${data_dir} \
    --output_dir=${model_dir} \
    --model=shake_shake \
    --hparams_set=shakeshake_basic \
    --train_steps=1000000 \
    --eval_steps=300 

